# Vaal AI Empire - Environment Configuration
# Copy this to .env and fill in your actual values

# Stripe API Keys (Get from: https://dashboard.stripe.com/apikeys)
STRIPE_PUBLISHABLE_KEY=pk_test_your_key_here
STRIPE_SECRET_KEY=sk_test_your_key_here

# Stripe Price IDs (Create in Dashboard: https://dashboard.stripe.com/products)
STARTER_PRICE_ID=price_starter_id_here
EMPIRE_PRICE_ID=price_empire_id_here

# Webhook secret (Get when setting up webhooks)
STRIPE_WEBHOOK_SECRET=whsec_your_secret_here

# Application settings
DOMAIN=http://localhost:4242
PORT=4242
NODE_ENV=development

# For production, update to:
# DOMAIN=https://vaalai.co.za
# STRIPE_PUBLISHABLE_KEY=pk_live_...
# STRIPE_SECRET_KEY=sk_live_...
# NODE_ENV=production